<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CashMachine.aWPF.Styles">
    
    <!--Colors-->
    <Color x:Key="ColorPrimary1DarkGrey">#2c2c2c</Color>
    <Color x:Key="ColorPrimary2LightGray">#d3d3d3</Color>
    
    <!--Brushes-->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource ColorPrimary1DarkGrey}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource ColorPrimary2LightGray}"/>

</ResourceDictionary>